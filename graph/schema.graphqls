type Project {
  id: ID!
  title: String!
  description: String!
  status: String!
  link: Boolean!
  url: String!
  giturl: String
  tags: [String!]!
}

input NewProject {
  title: String!
  description: String!
  status: String!
  link: Boolean!
  url: String!
  giturl: String
  tags: [String!]!
}

type Query {
  project(id: ID!): Project
}

type Mutation {
  createProject(input: NewProject!): Project
}
